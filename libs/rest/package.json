{
  "$schema": "http://json.schemastore.org/package",
  "name": "@cordis/rest",
  "description": "Cordis' REST utilities for the Discord API",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "version": "1.0.2",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build": "tsc",
    "bump-next": "pnpm version --git-tag-version=false $(jq --raw-output '.version' package.json)-next.$(git rev-parse --short HEAD).$(date +%s)"
  },
  "files": [
    "dist",
    "types",
    "!dist/*.tsbuildinfo",
    "!**/**.map",
    "!**/*.test.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordis-lib/cordis.git"
  },
  "contributors": [
    "didinele <didinele.dev@gmail.com> (https://github.com/didinele)",
    "Holo-Buckshot (https://github.com/Holo-Buckshot)",
    "Nico (https://github.com/zaida04)"
  ],
  "bugs": {
    "url": "https://github.com/cordis-lib/cordis/issues"
  },
  "homepage": "https://github.com/cordis-lib/cordis#readme",
  "devDependencies": {
    "@types/node": "^14.17.5",
    "fetch-blob": "^2.1.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@cordis/common": "workspace:^1.0.2",
    "@cordis/error": "workspace:^1.0.2",
    "abort-controller": "^3.0.0",
    "discord-api-types": "^0.22.0",
    "form-data": "^3.0.1",
    "node-fetch": "3.0.0-beta.9",
    "tslib": "^2.3.1"
  }
}
